input CreateTaskInput {
  text: String!
}

input UpdateTaskInput {
  id: ID!
  text: String
  status: Status
}

input CreateUserInput {
  name: String!
}

type Mutation {
  createTask(input: CreateTaskInput!): Task!
  updateTask(input: UpdateTaskInput): Task!
  createUser(input: CreateUserInput!): User!
}
